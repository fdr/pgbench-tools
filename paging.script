#!/usr/bin/gnuplot
set terminal svg size 800,600
set grid xtics ytics
set output "paging.svg"
set title "Page Statistics"
set xlabel "Time during test"
set ylabel "Pages"
set xdata time
set timefmt "%d-%m %H:%M:%S"
set format x "%M:%S"
plot "stat.stat" using 1:16 title "Page In" with lines, \
"stat.stat" using 1:17 title "Page Out" with lines

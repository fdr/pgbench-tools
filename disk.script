#!/usr/bin/gnuplot
set terminal svg size 800,600
set style data lines
set grid xtics ytics
set output "disk.svg"
set title "disk"
set xlabel "Time during test"
set ylabel "Disk Operations"
set xdata time
set timefmt "%d-%m %H:%M:%S"
set format x "%M:%S"
plot "stat.stat" using 1:22 title "disk reads" with lines, \
"stat.stat" using 1:23 title "disk writes" with lines

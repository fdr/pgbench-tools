#!/usr/bin/gnuplot
set terminal svg size 800,600
set style data lines
set grid xtics ytics
set output "system.svg"
set title "system"
set xlabel "Time during test"
set ylabel "Events"
set xdata time
set timefmt "%d-%m %H:%M:%S"
set format x "%M:%S"
plot "stat.stat" using 1:19 title "Interrupts" with lines, \
"stat.stat" using 1:20 title "Context Switches" with lines

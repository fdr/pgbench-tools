#!/usr/bin/gnuplot
set terminal svg size 800,600
set datafile separator ","
set output "swap.svg"
set style data lines
set grid xtics ytics
set title "Swap Statistics"
set xlabel "Time during test"
set ylabel "Pages"
min_time=system("echo $MIN_TIME")
max_time=system("echo $MAX_TIME")
max_swap=system("echo $MAX_SWAP")
set yrange [0:max_swap]
set xdata time
set timefmt "%s"
set format x "%M:%S"
set xrange [min_time:max_time]
plot "dstat.stat" using 1:12 title "Pages Used" with lines, \
"dstat.stat" using 1:13 title "Pages Free" with lines
